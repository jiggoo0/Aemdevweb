{
  "version": 1,
  "functions": {
    "/_middleware": {
      "runtime": "nodejs",
      "matchers": [
        {
          "regexp": "^(?:\\/(_next\\/data\\/[^/]{1,}))?(?:\\/((?!api|_next\\/static|_next\\/image|favicon.ico|sitemap.xml|robots.xml|.*\\.(?:svg|png|jpg|jpeg|webp|gif)$).*))(\\.json)?[\\/#\\?]?$",
          "originalSource": "/((?!api|_next/static|_next/image|favicon.ico|sitemap.xml|robots.xml|.*\\.(?:svg|png|jpg|jpeg|webp|gif)$).*)"
        }
      ]
    }
  }
}